<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="homeController">
   <children>
      <HBox>
         <children>
            <VBox>
               <children>
                  <Label fx:id="lblAzienda" text="Azienda" />
                  <Button fx:id="btnProdotti" mnemonicParsing="false" onAction="#doProdotti" text="Prodotti/Servizi" />
                  <Button fx:id="btnNews" mnemonicParsing="false" onAction="#doNews" text="News" />
                  <Button fx:id="btnProgetti" mnemonicParsing="false" onAction="#doProgetti" text="Progetti" />
                  <Button fx:id="btnBrevetti" mnemonicParsing="false" onAction="#doBrevetti" text="Brevetti" />
                  <Button fx:id="btnAppalti" mnemonicParsing="false" onAction="#doAppalti" text="Appalti" />
                  <Button fx:id="btnBilancio" mnemonicParsing="false" onAction="#doBilancio" text="Bilancio" />
                  <Button fx:id="btnArticoli" mnemonicParsing="false" onAction="#doArticoli" text="Articoli" />
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </VBox>
            <VBox>
               <children>
                  <Label text="Clicca sull'azienda per analizzarla meglio">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="1" />
                        <Button fx:id="btnCompetitor1" mnemonicParsing="false" onAction="#doCompetitor1" text="Competitor 1" />
                        <Button fx:id="btnCompara1" mnemonicParsing="false" onAction="#doCompara1" text="Compara" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="2" />
                        <Button fx:id="btnCompetitor2" mnemonicParsing="false" onAction="#doCompetitor2" text="Cmpetitor 2" />
                        <Button fx:id="btnCompara2" mnemonicParsing="false" onAction="#doCompara2" text="Compara" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="3" />
                        <Button fx:id="btnCompetitor3" mnemonicParsing="false" onAction="#doCompetitor3" text="Competitor 3" />
                        <Button fx:id="btnCompara3" mnemonicParsing="false" onAction="#doCompara3" text="Compara" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0">
                     <children>
                        <Label text="4" />
                        <Button fx:id="btnCompetitor4" mnemonicParsing="false" onAction="#doCompetitor4" text="Competitor 4" />
                        <Button fx:id="btnCompara4" mnemonicParsing="false" onAction="#doCompara4" text="Compara" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
